{
  "name": "blitz-coffee",
  "version": "0.1.5",
  "scripts": {
    "predev": "docker-compose up -d",
    "dev": "blitz dev",
    "build": "yarn migrate:deploy && blitz build",
    "start": "blitz start",
    "start:production": "blitz start --production --port $PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:fix": "rustywind --write . && eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "yarn jest",
    "test:watch": "jest --watch",
    "cy:open": "yarn run cypress open",
    "cy:run": "yarn run cypress run",
    "analyze": "ANALYZE=true yarn build",
    "tsc": "tsc --pretty",
    "migrate:dev": "blitz prisma migrate dev --preview-feature",
    "migrate:deploy": "blitz prisma migrate deploy --preview-feature"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.4",
    "@mozart409/footer": "1.0.2",
    "@prisma/client": "~2.30.2",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.3",
    "@tailwindcss/line-clamp": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "autoprefixer": "10.3.3",
    "bizcharts": "4.1.11",
    "blitz": "0.39.0",
    "dayjs": "1.10.7",
    "faker": "5.5.3",
    "final-form": "4.20.2",
    "ioredis": "4.27.9",
    "next-plausible": "2.1.1",
    "postcss": "8.3.6",
    "preview-email": "3.0.5",
    "prisma": "~2.30.2",
    "react": "0.0.0-experimental-6a589ad71",
    "react-avatar": "3.10.0",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.3",
    "react-final-form": "6.5.3",
    "tailwindcss": "2.2.9",
    "typescript": "~4.4.2",
    "yarn": "1.22.11",
    "zod": "^1.11.17"
  },
  "devDependencies": {
    "@types/faker": "5.5.8",
    "@types/ioredis": "4.27.1",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.19",
    "cypress": "8.3.1",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "prettier-plugin-prisma": "2.30.0",
    "pretty-quick": "3.1.1",
    "rustywind": "0.12.2"
  },
  "private": true
}
