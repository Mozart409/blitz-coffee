{
  "name": "blitz-coffee",
  "version": "0.1.0",
  "scripts": {
    "predev": "docker-compose up -d",
    "dev": "blitz dev",
    "build": "yarn migrate:deploy && blitz build",
    "start": "blitz start",
    "start:production": "blitz start --production --port $PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "analyze": "ANALYZE=true yarn build",
    "tsc": "tsc --pretty",
    "migrate:dev": "blitz prisma migrate dev --preview-feature",
    "migrate:deploy": "blitz prisma migrate deploy --preview-feature"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@mozart409/footer": "1.0.2",
    "@prisma/client": "~2.24.1",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.3",
    "@tailwindcss/line-clamp": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "autoprefixer": "10.2.6",
    "blitz": "0.36.4",
    "faker": "5.5.3",
    "final-form": "4.20.2",
    "postcss": "8.3.0",
    "prisma": "~2.24.1",
    "react": "0.0.0-experimental-6a589ad71",
    "react-avatar": "3.10.0",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.3",
    "react-final-form": "6.5.3",
    "tailwindcss": "2.1.4",
    "typescript": "~4.3.2",
    "zod": "3.1.0"
  },
  "devDependencies": {
    "@types/faker": "5.5.6",
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.9",
    "eslint": "7.28.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "prettier-plugin-prisma": "0.11.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4"
  },
  "private": true
}
