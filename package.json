{
  "name": "blitz-coffee",
  "version": "0.1.10",
  "scripts": {
    "predev": "docker-compose up -d",
    "dev": "blitz dev -H 0.0.0.0",
    "build": "yarn migrate:deploy && blitz build",
    "start": "blitz start",
    "start:production": "blitz start --production --port $PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:fix": "rustywind --write . && eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "yarn jest",
    "test:watch": "jest --watch",
    "cy:open": "yarn run cypress open",
    "cy:run": "yarn run cypress run",
    "analyze": "ANALYZE=true yarn build",
    "tsc": "tsc --pretty",
    "migrate:dev": "blitz prisma migrate dev --preview-feature",
    "migrate:deploy": "blitz prisma migrate deploy --preview-feature",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.2",
    "@heroicons/react": "1.0.5",
    "@next/bundle-analyzer": "12.0.7",
    "@prisma/client": "~3.7.0",
    "@relative-ci/agent": "3.0.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.0",
    "@tailwindcss/typography": "0.5.0",
    "autoprefixer": "10.4.1",
    "blitz": "0.44.4",
    "date-fns": "2.28.0",
    "faker": "5.5.3",
    "final-form": "4.20.6",
    "ioredis": "4.28.2",
    "lodash": "4.17.21",
    "logrocket": "2.1.3",
    "logrocket-react": "4.x",
    "next-compose-plugins": "2.2.1",
    "next-plausible": "3.1.4",
    "postcss": "8.4.5",
    "preview-email": "3.0.5",
    "prisma": "~3.7.0",
    "react": "0.0.0-experimental-4729ff6d1-20211202",
    "react-avatar": "4.0.0",
    "react-dom": "0.0.0-experimental-4729ff6d1-20211202",
    "react-error-boundary": "3.1.4",
    "react-final-form": "6.5.7",
    "react-resizable": "3.0.4",
    "recharts": "2.1.8",
    "tailwindcss": "3.0.9",
    "typescript": "~4.4.4",
    "webpack-stats-plugin": "1.0.3",
    "yarn": "1.22.17",
    "zod": "^1.11.17"
  },
  "devDependencies": {
    "@types/faker": "5.5.9",
    "@types/ioredis": "4.28.5",
    "@types/lodash": "4.14.178",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.38",
    "cypress": "9.1.1",
    "eslint": "8.3.0",
    "husky": "^7.0.0",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.6.0",
    "pretty-quick": "3.1.3",
    "rustywind": "0.12.2"
  },
  "private": true
}
