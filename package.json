{
  "name": "blitz-coffee",
  "version": "0.1.0",
  "scripts": {
    "predev": "docker-compose up -d",
    "dev": "blitz dev",
    "build": "yarn migrate:deploy && blitz build",
    "start": "blitz start",
    "start:production": "blitz start --production --port $PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "yarn jest",
    "test:watch": "jest --watch",
    "cy:open": "yarn run cypress open",
    "cy:run": "yarn run cypress run",
    "prepare": "husky install",
    "analyze": "ANALYZE=true yarn build",
    "tsc": "tsc --pretty",
    "migrate:dev": "blitz prisma migrate dev --preview-feature",
    "migrate:deploy": "blitz prisma migrate deploy --preview-feature"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.3.0",
    "@heroicons/react": "1.0.2",
    "@mozart409/footer": "1.0.2",
    "@prisma/client": "~2.27.0",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.3",
    "@tailwindcss/line-clamp": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "@visx/axis": "1.17.1",
    "@visx/curve": "1.7.0",
    "@visx/gradient": "1.7.0",
    "@visx/grid": "1.17.1",
    "@visx/group": "1.17.1",
    "@visx/legend": "1.17.1",
    "@visx/marker": "1.17.1",
    "@visx/mock-data": "1.7.0",
    "@visx/responsive": "1.10.1",
    "@visx/scale": "1.14.0",
    "@visx/shape": "1.17.1",
    "@visx/tooltip": "1.17.1",
    "add": "2.0.6",
    "autoprefixer": "10.3.1",
    "blitz": "0.38.5",
    "d3-array": "3.0.1",
    "faker": "5.5.3",
    "final-form": "4.20.2",
    "ioredis": "4.27.6",
    "next-plausible": "2.0.0",
    "postcss": "8.3.5",
    "prisma": "~2.27.0",
    "react": "0.0.0-experimental-6a589ad71",
    "react-avatar": "3.10.0",
    "react-charts": "3.0.0-beta.32",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.3",
    "react-final-form": "6.5.3",
    "tailwindcss": "2.2.4",
    "typescript": "~4.3.5",
    "yarn": "1.22.11",
    "zod": "^1.11.17"
  },
  "devDependencies": {
    "@types/faker": "5.5.7",
    "@types/ioredis": "4.26.6",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.14",
    "cypress": "8.0.0",
    "eslint": "7.31.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "prettier-plugin-prisma": "0.14.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.4"
  },
  "private": true
}
